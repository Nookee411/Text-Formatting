n', sans-serif;\r\n  font-size: 14px;\r\n  line-height: 18px;\r\n  color: #e8eaed;\r\n  border-radius: 12px;\r\n  height: 620px;\r\n  min-width: 100%;\r\n  outline: none;\r\n  border: none;\r\n}\r\n.textBox::-webkit-scrollbar {\r\n  display: none;\r\n  width: 10px;\r\n}\r\n\r\n.buttonConvert {\r\n  font-size: 1em;\r\n  font-weight: bold;\r\n  width: 100%;\r\n  padding: 1em;\r\n  transition: background-color 0.3s;\r\n  outline: none;\r\n  border: none;\r\n  border-radius: 8px;\r\n}\r\n\r\n.buttonConvert:hover {\r\n  background-color: hsla(0, 0%, 75%);\r\n}\r\n.buttonConvert:active {\r\n  box-shadow: none;\r\n  background-color: white;\r\n}\r\n\r\n.Counter {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  outline: none;\r\n  border: none;\r\n  text-align: center;\r\n  background-color: inherit;\r\n}\r\n#timerOutput {\r\n  display: none;\r\n}\r\n.switch {\r\n  position: fixed;\r\n  bottom: 1%;\r\n  right: 1%;\r\n  width: 2.5rem;\r\n  height: 2.5rem;\r\n  line-height: 2.5rem;\r\n  text-align: center;\r\n  border-radius: 50%;\r\n  transition-duration: 0.5s;\r\n  background-color: #fff;\r\n  color: #333;\r\n  user-select: none;\r\n}\r\n\r\n.switch:hover {\r\n  background-color: #333;\r\n  color: #fff;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"cd2278b3054d343d8d60dd57f41830d7","cacheData":{"env":{}}}
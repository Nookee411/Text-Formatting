g\");\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n\r\n.container{\r\n    position:           absolute;\r\n    top:                10%;\r\n    display:            flex;\r\n    flex-wrap:          wrap;\r\n    margin:             auto;\r\n    max-width:          65%;\r\n    min-width:          350px;\r\n\r\n    background-color: rgba(128,128,128,0.8);\r\n    padding:            10px;\r\n}\r\n\r\n.input, .output{\r\n    position:       relative;\r\n    float:          left;\r\n    display:        block;\r\n    padding-top:    20px;\r\n    margin:         0;\r\n    text-align:     center;\r\n    flex:1;\r\n    overflow:       hidden;\r\n    min-height:     auto;\r\n}\r\n.output{\r\n    display:        flex;\r\n    flex-direction: column;\r\n    min-width: 300px;\r\n}\r\n.input{\r\n    min-width: 350px;\r\n}\r\n\r\n.output div{\r\n    margin:             0 auto 10px auto;\r\n    line-height:        3em;\r\n    min-width:              90%;\r\n    background-color: wheat;\r\n    text-align:         center;\r\n    overflow:           auto;\r\n}\r\n\r\n.textBox{\r\n\r\n    color:        black;\r\n    resize:         none;\r\n    height:         623px;\r\n    width:          340px;\r\n    outline:        none;\r\n    margin:         auto;\r\n    font-family:    'Proxima Nova Regular',\"Times New Roman\",sans-serif;\r\n    font-size:      14px;\r\n    line-height:    18px;\r\n\r\n}\r\n.textBox::-webkit-scrollbar{\r\n    display: none;\r\n    width: 10px;\r\n}\r\n\r\n@media (min-width: 300px) {\r\n    .container{\r\n        top:0;\r\n    }\r\n    \r\n}\r\n\r\n.buttonConvert{\r\n    font-size:      1em;\r\n    height:         30px;\r\n    width:          50%;\r\n    margin:         10px;\r\n    transition:     background-color 0.3s;\r\n    outline:        none;\r\n    border:         none;\r\n}\r\n\r\n.timer{\r\n    font-family:    \"Open Sans\", sans-serif;\r\n    overflow:       hidden;\r\n    top:            345px;\r\n    height:         100px;\r\n}\r\n.time
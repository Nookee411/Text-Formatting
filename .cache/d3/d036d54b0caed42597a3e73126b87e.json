ut{\r\n    display:        flex;\r\n    flex-direction: column;\r\n    min-width: 300px;\r\n}\r\n.input{\r\n    min-width: 350px;\r\n}\r\n\r\n.output div{\r\n    margin:             0 auto 10px auto;\r\n    line-height:        3em;\r\n    min-width:              90%;\r\n    background-color: wheat;\r\n    text-align:         center;\r\n    overflow:           auto;\r\n}\r\n\r\n.textBox{\r\n\r\n    color:        black;\r\n    resize:         none;\r\n    height:         623px;\r\n    width:          340px;\r\n    outline:        none;\r\n    margin:         auto;\r\n    font-family:    'Proxima Nova Regular',\"Times New Roman\",sans-serif;\r\n    font-size:      14px;\r\n    line-height:    18px;\r\n\r\n}\r\n.textBox::-webkit-scrollbar{\r\n    display: none;\r\n    width: 10px;\r\n}\r\n\r\n@media (max-width: 1000px) {\r\n    body{\r\n        overflow-x: hidden;\r\n    }\r\n    .container{\r\n        top: 0;\r\n    }\r\n\r\n}\r\n\r\n.buttonConvert{\r\n    font-size:      1em;\r\n    height:         30px;\r\n    width:          340px;\r\n    transition:     background-color 0.3s;\r\n    outline:        none;\r\n    border:         none;\r\n}\r\n\r\n.timer{\r\n    font-family:    \"Open Sans\", sans-serif;\r\n    overflow:       hidden;\r\n    top:            345px;\r\n    height:         100px;\r\n}\r\n.timer span{\r\n    display:    block;\r\n}\r\n.timer button{\r\n    background-color: rgb(243, 243, 243);\r\n    width:              100%;\r\n    margin:             0;\r\n    height:             52%;\r\n}\r\n.Counter{\r\n    font-size:          20px;\r\n    font-weight:        bold;\r\n    outline:            none;\r\n    border:             none;\r\n    text-align:         center;\r\n    background-color:   inherit;\r\n}\r\n#timerOutput{\r\n    display : none;\r\n}\r\n@font-face {\r\n    font-family: 'Proxima Nova Regular';\r\n    src: url('ProximaNova-Regular.eot');\r\n    src: url('ProximaNova-Regular.eot?#iefix') format('embedded-opentype'),\r\n    url('ProximaNova-Regular.woff') format('woff'),\r\n    url('ProximaNova-Regular.ttf') format('truetype');\r\n    font-weight: normal;\r\n    font-style: normal;\r\n}\r\n\r\n.buttonConvert, .output div{\r\n    background-color: lightgray;\r\n}\r\n.buttonConvert:hover{\r\n    box-shadow:         2px 2px 2px 0 black;\r\n    background-color: rgba(128,128,128,1);\r\n}\r\n.buttonConvert:active{\r\n    box-shadow:         none;\r\n    background-color: white;\r\n}\r\n\r\n.switch{\r\n    display: flex;\r\n    position: fixed;\r\n    bottom: 0;\r\n    right: 0;\r\n    width: 2rem;\r\n    height: 2rem;\r\n    justify-content: center;\r\n    align-items: center;\r\n    transition-duration: .5s;\r\n    background-color: #fff;\r\n    color: #333;\r\n}\r\n\r\n.switch:hover{\r\n    background-color: #333;\r\n    color: #fff;\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"9bdd0fa0a0488c82f3fc0a88d5d08d16","cacheData":{"env":{}}}